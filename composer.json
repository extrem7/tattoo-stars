{
    "name": "extrem7/tattoo-stars",
    "type": "project",
    "description": "The TattooStars api and admin-panel.",
    "keywords": [
        "rest-api",
        "admin-panel",
        "laravel"
    ],
    "license": "MIT",
    "require": {
      "php": "^7.4.4|^8.0",
      "ext-gd": "*",
      "artesaos/seotools": "^0.20.0",
      "barryvdh/laravel-debugbar": "^3.5",
      "barryvdh/laravel-ide-helper": "^2.9",
      "dyrynda/laravel-cascade-soft-deletes": "^4.1",
      "fideloper/proxy": "^4.4",
      "fruitcake/laravel-cors": "^2.0",
      "funkjedi/composer-include-files": "^1.1",
      "guzzlehttp/guzzle": "^7.0.1",
      "inertiajs/inertia-laravel": "^0.3.6",
      "laravel/framework": "^8.12",
      "laravel/sanctum": "^2.8",
      "laravel/telescope": "^4.4",
      "laravel/tinker": "^2.5",
      "nwidart/laravel-modules": "^8.2",
      "pbmedia/laravel-ffmpeg": "^7.5",
      "php-ffmpeg/php-ffmpeg": "^0.17.0",
      "schuppo/password-strength": "^2.3",
      "spatie/eloquent-sortable": "^3.11",
      "spatie/laravel-medialibrary": "^9.0.0",
      "spatie/laravel-permission": "^3.18",
      "spatie/laravel-translatable": "^4.6",
      "stichoza/google-translate-php": "^4.1",
      "tightenco/ziggy": "^1.0",
      "torann/geoip": "^3.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
